<?xml version="1.0" encoding="UTF-8"?>
<OpenSearchDescription xmlns="http://a9.com/-/spec/opensearch/1.1/"
	xmlns:geo="http://a9.com/-/opensearch/extensions/geo/1.0/"
	xmlns:time="http://a9.com/-/opensearch/extensions/time/1.0/"
	xmlns:dco="http://data.deepcarbon.net/browsers/ontologies/dco-s2s.ttl#"
	xmlns:core="http://escience.rpi.edu/ontology/sesf/s2s-core/4/0/">

  <ShortName>DCO Data Portal Field Study Search</ShortName>
  <Description>DCO Data Portal Field Study Search Service. This opensearch
  document describes how to call the various RESTful services in order
  to fill s2s facets (field study participants, DCO communities the
  field studies are related to, DCO groups the field studies are related
  to, the grants that funded the field study, start date of the field
  study, and end date of the field study), the result set, and a map
  displaying the locations of the result field studies.</Description>
  <Tags></Tags>
  <Contact>web@mail.deepcarbon.net</Contact>
  <Url type="application/json" rel="dco:ParticipantsQuery" template="http://data.deepcarbon.net/browsers/opensearch/services/field_studies/search.php?request=participants&amp;communities={dco:Communities?}&amp;groups={dco:Groups?}&amp;grants={dco:Grants?}&amp;startDate={dco:StartDate?}&amp;endDate={dco:EndDate?}&amp;reportingYears={dco:ReportingYears?}" />
  <Url type="application/json" rel="dco:CommunitiesQuery" template="http://data.deepcarbon.net/browsers/opensearch/services/field_studies/search.php?request=communities&amp;participants={dco:Participants?}&amp;groups={dco:Groups?}&amp;grants={dco:Grants?}&amp;startDate={dco:StartDate?}&amp;endDate={dco:EndDate?}&amp;reportingYears={dco:ReportingYears?}" />
  <Url type="application/json" rel="dco:GroupsQuery" template="http://data.deepcarbon.net/browsers/opensearch/services/field_studies/search.php?request=groups&amp;participants={dco:Participants?}&amp;communities={dco:Communities?}&amp;grants={dco:Grants?}&amp;startDate={dco:StartDate?}&amp;endDate={dco:EndDate?}&amp;reportingYears={dco:ReportingYears?}" />
  <Url type="application/json" rel="dco:GrantsQuery" template="http://data.deepcarbon.net/browsers/opensearch/services/field_studies/search.php?request=grants&amp;participants={dco:Participants?}&amp;communities={dco:Communities?}&amp;groups={dco:Groups?}&amp;startDate={dco:StartDate?}&amp;endDate={dco:EndDate?}&amp;reportingYears={dco:ReportingYears?}" />
  <Url type="application/json" rel="dco:StartDateQuery" template="http://data.deepcarbon.net/browsers/opensearch/services/field_studies/search.php?request=startDate&amp;participants={dco:Participants?}&amp;communities={dco:Communities?}&amp;groups={dco:Groups?}&amp;grants={dco:Grants?}&amp;endDate={dco:EndDate?}&amp;reportingYears={dco:ReportingYears?}" />
  <Url type="application/json" rel="dco:EndDateQuery" template="http://data.deepcarbon.net/browsers/opensearch/services/field_studies/search.php?request=EndDate&amp;participants={dco:Participants?}&amp;communities={dco:Communities?}&amp;groups={dco:Groups?}&amp;grants={dco:Grants?}&amp;startDate={dco:StartDate?}&amp;reportingYears={dco:ReportingYears?}" />
  <Url type="application/json" rel="dco:ReportingYearsQuery" template="http://data.deepcarbon.net/browsers/opensearch/services/field_studies/search.php?request=reportingYears&amp;participants={dco:Participants?}&amp;communities={dco:Communities?}&amp;groups={dco:Groups?}&amp;grants={dco:Grants?}&amp;startDate={dco:StartDate?}&amp;endDate={dco:EndDate?}" />
    <Url type="text/html" rel="dco:ResultsListQuery" template="http://data.deepcarbon.net/browsers/opensearch/services/field_studies/search.php?request=field_studies&amp;participants={dco:Participants?}&amp;communities={dco:Communities?}&amp;groups={dco:Groups?}&amp;grants={dco:Grants?}&amp;startDate={dco:StartDate?}&amp;endDate={dco:EndDate?}&amp;reportingYears={dco:ReportingYears?}&amp;limit={count}&amp;offset={startIndex}" />
  <Url type="application/json" rel="dco:ResultsMapQuery" template="http://data.deepcarbon.net/browsers/opensearch/services/field_studies/search.php?request=field_studies_map&amp;participants={dco:Participants?}&amp;communities={dco:Communities?}&amp;groups={dco:Groups?}&amp;grants={dco:Grants?}&amp;startDate={dco:StartDate?}&amp;endDate={dco:EndDate?}&amp;reportingYears={dco:ReportingYears?}&amp;limit={count}&amp;offset={startIndex}" />
  <LongName>DCO Data Portal Field Study Search Service</LongName>
  <Developer>DCO Data Science Team</Developer>
  <Attribution></Attribution>
  <SyndicationRight>open</SyndicationRight>
  <AdultContent>false</AdultContent>
  <Language>en-us</Language>
  <OutputEncoding>UTF-8</OutputEncoding>
  <InputEncoding>UTF-8</InputEncoding>
</OpenSearchDescription>


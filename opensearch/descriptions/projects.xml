<?xml version="1.0" encoding="UTF-8"?>
<OpenSearchDescription
        xmlns="http://a9.com/-/spec/opensearch/1.1/"
        xmlns:dco="https://data.deepcarbon.net/browsers/ontologies/dco-s2s.ttl#">

  <ShortName>DCO Data Portal Project Search</ShortName>
  <Description>DCO Data Portal Project Search Service, displaying the
  list of projects that match a set of constraints. This opensearch
  document describes how to call the various RESTful services in order
  to fill s2s facets (participants in the projects, DCO communities that
  the projects relate to, DCO groups that the projects relate to, the
  grants that fund the projects, start date of the project, and end date
  of the projects) and the result set listing the projects that match
  the constraints.</Description>
  <Tags></Tags>
  <Contact>web@mail.deepcarbon.net</Contact>
  <Url type="application/json" rel="dco:ParticipantsQuery" template="https://data.deepcarbon.net/browsers/opensearch/services/projects/search.php?request=participants&amp;communities={dco:Communities?}&amp;groups={dco:Groups?}&amp;grants={dco:Grants?}&amp;startDate={dco:StartDate?}&amp;endDate={dco:EndDate?}&amp;reportingYears={dco:ReportingYears?}" />
  <Url type="application/json" rel="dco:CommunitiesQuery" template="https://data.deepcarbon.net/browsers/opensearch/services/projects/search.php?request=communities&amp;participants={dco:Participants?}&amp;groups={dco:Groups?}&amp;grants={dco:Grants?}&amp;startDate={dco:StartDate?}&amp;endDate={dco:EndDate?}&amp;reportingYears={dco:ReportingYears?}" />
  <Url type="application/json" rel="dco:GroupsQuery" template="https://data.deepcarbon.net/browsers/opensearch/services/projects/search.php?request=groups&amp;participants={dco:Participants?}&amp;communities={dco:Communities?}&amp;grants={dco:Grants?}&amp;startDate={dco:StartDate?}&amp;endDate={dco:EndDate?}&amp;reportingYears={dco:ReportingYears?}" />
  <Url type="application/json" rel="dco:GrantsQuery" template="https://data.deepcarbon.net/browsers/opensearch/services/projects/search.php?request=grants&amp;participants={dco:Participants?}&amp;communities={dco:Communities?}&amp;groups={dco:Groups?}&amp;startDate={dco:StartDate?}&amp;endDate={dco:EndDate?}&amp;reportingYears={dco:ReportingYears?}" />
  <Url type="application/json" rel="dco:StartDateQuery" template="https://data.deepcarbon.net/browsers/opensearch/services/projects/search.php?request=startDate&amp;participants={dco:Participants?}&amp;communities={dco:Communities?}&amp;grants={dco:Grants?}&amp;groups={dco:Groups?}&amp;endDate={dco:EndDate?}&amp;reportingYears={dco:ReportingYears?}" />
  <Url type="application/json" rel="dco:EndDateQuery" template="https://data.deepcarbon.net/browsers/opensearch/services/projects/search.php?request=EndDate&amp;participants={dco:Participants?}&amp;communities={dco:Communities?}&amp;grants={dco:Grants?}&amp;groups={dco:Groups?}&amp;startDate={dco:StartDate?}&amp;reportingYears={dco:ReportingYears?}" />
    <Url type="application/json" rel="dco:ReportingYearsQuery" template="https://data.deepcarbon.net/browsers/opensearch/services/projects/search.php?request=reportingYears&amp;participants={dco:Participants?}&amp;communities={dco:Communities?}&amp;grants={dco:Grants?}&amp;groups={dco:Groups?}&amp;startDate={dco:StartDate?}&amp;endDate={dco:EndDate?}" />
  <Url type="text/html" rel="dco:ResultsListQuery" template="https://data.deepcarbon.net/browsers/opensearch/services/projects/search.php?request=projects&amp;participants={dco:Participants?}&amp;communities={dco:Communities?}&amp;grants={dco:Grants?}&amp;groups={dco:Groups?}&amp;startDate={dco:StartDate?}&amp;endDate={dco:EndDate?}&amp;reportingYears={dco:ReportingYears?}&amp;limit={count}&amp;offset={startIndex}" />
  <LongName>DCO Data Portal Project Search Service</LongName>
  <Developer>DCO Data Science Team</Developer>
  <Attribution></Attribution>
  <SyndicationRight>open</SyndicationRight>
  <AdultContent>false</AdultContent>
  <Language>en-us</Language>
  <OutputEncoding>UTF-8</OutputEncoding>
  <InputEncoding>UTF-8</InputEncoding>
</OpenSearchDescription>


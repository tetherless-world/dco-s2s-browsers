<?xml version="1.0" encoding="UTF-8"?>
<OpenSearchDescription
        xmlns="http://a9.com/-/spec/opensearch/1.1/"
        xmlns:dco="https://data.deepcarbon.net/browsers/ontologies/dco-s2s.ttl#">

  <ShortName>DCO Data Portal Field Study Search</ShortName>
  <Description>DCO Data Portal Field Study Search Service displaying
  field studies that are open to journalists. This opensearch
  document describes how to call the various RESTful services in order
  to fill s2s facets (field study participants, DCO communities the
  field studies are related to, DCO groups the field studies are related
  to, the grants that funded the field studies, start date of the field
  studies, and end date of the field studies), and a map displaying the
  locations of the resulting field studies that journalists are pointed
  to.</Description>
  <Tags></Tags>
  <Contact>web@mail.deepcarbon.net</Contact>
  <Url type="application/json" rel="dco:ParticipantsQuery" template="https://data.deepcarbon.net/browsers/opensearch/services/field_studies/search.php?request=participants&amp;communities={dco:Communities?}&amp;groups={dco:Groups?}&amp;grants={dco:Grants?}&amp;startDate={dco:StartDate?}&amp;endDate={dco:EndDate?}&amp;reportingYears={dco:ReportingYears?}" />
  <Url type="application/json" rel="dco:CommunitiesQuery" template="https://data.deepcarbon.net/browsers/opensearch/services/field_studies/search.php?request=communities&amp;participants={dco:Participants?}&amp;groups={dco:Groups?}&amp;grants={dco:Grants?}&amp;startDate={dco:StartDate?}&amp;endDate={dco:EndDate?}&amp;reportingYears={dco:ReportingYears?}" />
  <Url type="application/json" rel="dco:GroupsQuery" template="https://data.deepcarbon.net/browsers/opensearch/services/field_studies/search.php?request=groups&amp;participants={dco:Participants?}&amp;communities={dco:Communities?}&amp;grants={dco:Grants?}&amp;startDate={dco:StartDate?}&amp;endDate={dco:EndDate?}&amp;reportingYears={dco:ReportingYears?}" />
  <Url type="application/json" rel="dco:GrantsQuery" template="https://data.deepcarbon.net/browsers/opensearch/services/field_studies/search.php?request=grants&amp;participants={dco:Participants?}&amp;communities={dco:Communities?}&amp;groups={dco:Groups?}&amp;startDate={dco:StartDate?}&amp;endDate={dco:EndDate?}&amp;reportingYears={dco:ReportingYears?}" />
  <Url type="application/json" rel="dco:StartDateQuery" template="https://data.deepcarbon.net/browsers/opensearch/services/field_studies/search.php?request=startDate&amp;participants={dco:Participants?}&amp;communities={dco:Communities?}&amp;groups={dco:Groups?}&amp;grants={dco:Grants?}&amp;endDate={dco:EndDate?}&amp;reportingYears={dco:ReportingYears?}" />
  <Url type="application/json" rel="dco:EndDateQuery" template="https://data.deepcarbon.net/browsers/opensearch/services/field_studies/search.php?request=EndDate&amp;participants={dco:Participants?}&amp;communities={dco:Communities?}&amp;groups={dco:Groups?}&amp;grants={dco:Grants?}&amp;startDate={dco:StartDate?}&amp;reportingYears={dco:ReportingYears?}" />
    <Url type="application/json" rel="dco:ReportingYearsQuery" template="https://data.deepcarbon.net/browsers/opensearch/services/field_studies/search.php?request=reportingYears&amp;participants={dco:Participants?}&amp;communities={dco:Communities?}&amp;groups={dco:Groups?}&amp;grants={dco:Grants?}&amp;startDate={dco:StartDate?}&amp;endDate={dco:EndDate?}" />
  <Url type="application/json" rel="dco:ResultsMapQuery" template="https://data.deepcarbon.net/browsers/opensearch/services/field_studies/search.php?request=field_studies_open_to_journalists_map&amp;participants={dco:Participants?}&amp;communities={dco:Communities?}&amp;groups={dco:Groups?}&amp;grants={dco:Grants?}&amp;startDate={dco:StartDate?}&amp;endDate={dco:EndDate?}&amp;reportingYears={dco:ReportingYears?}&amp;limit={count}&amp;offset={startIndex}" />
  <LongName>DCO Data Portal Field Study Search Service</LongName>
  <Developer>DCO Data Science Team</Developer>
  <Attribution></Attribution>
  <SyndicationRight>open</SyndicationRight>
  <AdultContent>false</AdultContent>
  <Language>en-us</Language>
  <OutputEncoding>UTF-8</OutputEncoding>
  <InputEncoding>UTF-8</InputEncoding>
</OpenSearchDescription>


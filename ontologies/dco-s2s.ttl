:SearchTerms
    core:hasDelimiter ";"^^xsd:string ;
    a s2s:Input, owl:Thing ;
    rdfs:comment "A search parameter for free-text keyword search."^^xsd:string ;
    rdfs:label "Search Terms (Title or Abstract)"^^xsd:string .


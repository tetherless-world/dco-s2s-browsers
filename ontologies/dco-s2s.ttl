geo:box
    core:hasDelimiter ";"^^xsd:string ;
    a s2s:Input, owl:Thing ;
    rdfs:comment "A search parameter for data realted to any input bounding box."^^xsd:string ;
    rdfs:label "Geo Bounding Box"^^xsd:string .

